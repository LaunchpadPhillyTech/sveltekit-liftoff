{
  "name": "gy6ai-sveltekit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "prisma generate && vite build",
    "dev": "vite dev --port 3000 --host localhost --open",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "reset": "rm -rf node_modules .svelte-kit && rm -rf package-lock.json && rm -rf pnpm-lock.yaml && rm -rf yarn.lock && npx prisma generate && npx prisma db push && npx prisma migrate dev && pnpm install && vercel build"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.8.1",
    "@sveltejs/kit": "^2.27.0",
    "@sveltejs/vite-plugin-svelte": "^2.5.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "prisma": "^6.13.0",
    "sass": "^1.69.5",
    "svelte": "^5.37.3",
    "svelte-check": "^3.4.3",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@prisma/client": "6.13.0",
    "@sendgrid/mail": "^7.7.0",
    "lucide-svelte": "^0.294.0",
    "path": "^0.12.7",
    "zod": "^3.22.4"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
